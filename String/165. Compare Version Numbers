class Solution {
public:
    int compareVersion(string version1, string version2) {
        int n=version1.length(), m=version2.length(), i=0, j=0;
        while(i<n && j<m)
        {
            string temp1="", temp2="";
            while(i<n && version1[i]!='.')
            {
                temp1+=version1[i];
                i++;
            }
            while(j<m && version2[j]!='.')
            {
                temp2+=version2[j];
                j++;
            }
            int num1=stoi(temp1), num2=stoi(temp2);
            if(num1<num2)
                return -1;
            else if(num1>num2)
                return 1;
            else{
                i++;
                j++;
            }
        }
        while(j<m)
        {
                string temp2="";
                while(j<m && version2[j]!='.')
                {
                    temp2+=version2[j];
                    j++;
                }
                int num2=stoi(temp2);
                if(num2>0)
                    return -1;
                j++;
        }
        while(i<n)
        {
            string temp1="";
            while(i<n && version1[i]!='.')
            {
                temp1+=version1[i];
                i++;
            }
            int num1=stoi(temp1);
            if(num1>0)
                return 1; 
            i++;    
        }
        return 0;
    }
};
